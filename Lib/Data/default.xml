        </articulation>
    </instrument>
    <instrument>
        <instrument-name>Long</instrument-name>
        <articulation>
            <articulation-name>Tenuto</articulation-name>
            <action name="apply_articulation_from_preset">3</action>
        </articulation>
        <articulation>
            <articulation-name>Lour√©</articulation-name>
            <action name="apply_articulation_from_preset">4</action>
        </articulation>
    </instrument>
