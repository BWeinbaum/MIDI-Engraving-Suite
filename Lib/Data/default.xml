<?xml version="1.0" encoding="UTF-8"?>
<plugin>
    <plugin-name>Default</plugin-name>
    <property name="hide_name_in_preview">true</property>
    <property name="prioritize_in_order">true</property>
    <instrument>
        <instrument-name>No Articulation</instrument-name>
        <articulation>
